-- Farm
FarmTab:CreateToggle({Name = "Auto Farm (Safe NO Traps)", CurrentValue = false, Callback = function(v) AutoFarm = v; ToggleNoClip(AutoFarm) end})
FarmTab:CreateToggle({Name = "Kill Aura Solo", CurrentValue = false, Callback = function(v) KillAura = v end})
FarmTab:CreateToggle({Name = "Auto Collect", CurrentValue = false, Callback = function(v) AutoCollect = v end})
FarmTab:CreateToggle({Name = "No Sword Cooldown (Ultra Spam)", CurrentValue = false, Callback = function(v) NoCooldown = v end})
FarmTab:CreateSlider({Name = "Aura Range", Range = {100, 500}, Increment = 50, CurrentValue = 300, Callback = function(v) AuraRange = v end})
FarmTab:CreateSlider({Name = "Underground Depth", Range = {20, 60}, Increment = 5, CurrentValue = 35, Callback = function(v) UndergroundDepth = -v end})
