-- Movement
MoveTab:CreateToggle({Name = "Fly (WASD+Space/Shift)", CurrentValue = false, Callback = function(v) ToggleFly(v) end})
MoveTab:CreateSlider({Name = "Fly Speed", Range = {16, 300}, Increment = 10, CurrentValue = 50, Callback = function(v) FlySpeed = v end})
MoveTab:CreateSlider({Name = "Walk Speed", Range = {16, 200}, Increment = 5, CurrentValue = 50, Callback = function(v) WalkSpeedVal = v end})
MoveTab:CreateSlider({Name = "Jump Power", Range = {50, 300}, Increment = 10, CurrentValue = 100, Callback = function(v) JumpPowerVal = v end})
MoveTab:CreateToggle({Name = "NoClip", CurrentValue = false, Callback = function(v) ToggleNoClip(v) end})
MoveTab:CreateButton({Name = "TP Mouse", Callback = function()
   local mouse = Player:GetMouse()
   if mouse.Hit then RootPart.CFrame = mouse.Hit.Position + Vector3.new(0,5,0) end
end})

-- Extras + God
ExtraTab:CreateToggle({Name = "God Mode (InvencÃ­vel Real)", CurrentValue = false, Callback = function(v) ToggleGodMode(v) end})
ExtraTab:CreateToggle({Name = "Auto Parry/Block", CurrentValue = false, Callback = function(v) AutoParry = v end})
ExtraTab:CreateToggle({Name = "Fullbright", CurrentValue = false, Callback = function(v) FullbrightT = v end})
ExtraTab:CreateToggle({Name = "Anti-AFK", CurrentValue = false, Callback = function(v) AntiAFK = v end})
ExtraTab:CreateButton({Name = "Rejoin", Callback = function() TeleportService:Teleport(game.PlaceId, Player) end})
ExtraTab:CreateButton({Name = "Destroy GUI", Callback = function() Rayfield:Destroy() end})

